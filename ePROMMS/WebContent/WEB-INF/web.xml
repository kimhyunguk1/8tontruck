<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
	<display-name>PromesService</display-name>
	<servlet>
		<servlet-name>PromesService</servlet-name>
		<servlet-class>
			kr.re.etri.lifeinfomatics.promes.main.PromesServerServlet
		</servlet-class>
		<load-on-startup>100</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>PromesService</servlet-name>
		<url-pattern>/PromesServerServlet</url-pattern>
	</servlet-mapping>
</web-app>
